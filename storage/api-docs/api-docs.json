    },
    "components": {
        "schemas": {
            "LoginRequest": {
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "john@example.com"
                    },
                    "password": {
                        "type": "string",
                        "format": "password",
                        "example": "password123"
                    }
                },
                "type": "object"
            },
            "AuthResponse": {
                "properties": {
                    "user": {
                        "$ref": "#/components/schemas/User"
                    },
                    "token": {
                        "type": "string",
                        "example": "1|laravel_sanctum_token..."
                    }
                },
                "type": "object"
            },
            "MessageResponse": {
                "properties": {
                    "message": {
                        "type": "string",
                        "example": "Operation successful"
                    }
                },
                "type": "object"
            },
            "Error": {
                "required": [
                    "message"
                ],
                "properties": {
                    "message": {
                        "type": "string",
                        "example": "Error message"
                    }
                },
                "type": "object"
            },
            "OrderRequest": {
                "required": [
                    "pickup_location",
                    "delivery_location",
                    "cargo_details",
                    "pickup_time",
                    "delivery_time"
                ],
                "properties": {
                    "pickup_location": {
                        "type": "string",
                        "example": "123 Pickup St"
                    },
                    "delivery_location": {
                        "type": "string",
                        "example": "456 Delivery Ave"
                    },
                    "cargo_details": {
                        "type": "object",
                        "example": {
                            "weight": "50kg",
                            "dimensions": "100x50x75cm"
                        }
                    },
                    "pickup_time": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "delivery_time": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            },
            "Order": {
                "allOf": [
                    {
                        "$ref": "#/components/schemas/OrderRequest"
                    },
                    {
                        "properties": {
                            "id": {
                                "type": "integer",
                                "format": "int64",
                                "example": 1
                            },
                            "user_id": {
                                "type": "integer",
                                "format": "int64",
                                "example": 1
                            },
                            "status": {
                                "type": "string",
                                "enum": [
                                    "pending",
                                    "in_progress",
                                    "completed",
                                    "cancelled"
                                ],
                                "example": "pending"
                            },
                            "created_at": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "updated_at": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "deleted_at": {
                                "type": "string",
                                "format": "date-time",
                                "nullable": true
                            }
                        },
                        "type": "object"
                    }
                ]
            },
            "OrderCollection": {
                "properties": {
                    "data": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Order"
                        }
                    },
                    "meta": {
                        "properties": {
                            "current_page": {
                                "type": "integer"
                            },
                            "from": {
                                "type": "integer"
                            },
                            "last_page": {
                                "type": "integer"
                            },
                            "per_page": {
                                "type": "integer"
                            },
                            "total": {
                                "type": "integer"
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            },
            "CreateOrderRequest": {
                "required": [
                    "pickup_location",
                    "delivery_location",
                    "cargo_details",
                    "pickup_time",
                    "delivery_time"
                ],
                "properties": {
                    "pickup_location": {
                        "type": "string",
                        "example": "123 Pickup St"
                    },
                    "delivery_location": {
                        "type": "string",
                        "example": "456 Delivery Ave"
                    },
                    "cargo_details": {
                        "required": [
                            "weight",
                            "dimensions"
                        ],
                        "properties": {
                            "weight": {
                                "type": "number",
                                "format": "float",
                                "maximum": 5000,
                                "minimum": 1,
                                "example": 100
                            },
                            "dimensions": {
                                "required": [
                                    "length",
                                    "width",
                                    "height"
                                ],
                                "properties": {
                                    "length": {
                                        "type": "number",
                                        "format": "float",
                                        "minimum": 1,
                                        "example": 10
                                    },
                                    "width": {
                                        "type": "number",
                                        "format": "float",
                                        "minimum": 1,
                                        "example": 10
                                    },
                                    "height": {
                                        "type": "number",
                                        "format": "float",
                                        "minimum": 1,
                                        "example": 10
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "type": "object"
                    },
                    "pickup_time": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2023-10-01T10:00:00Z"
                    },
                    "delivery_time": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2023-10-02T10:00:00Z"
                    }
                },
                "type": "object"
            },
            "UserRequest": {
                "required": [
                    "name",
                    "email",
                    "password",
